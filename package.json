{
  "name": "app-base",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config ./proxy.conf.json",
    "build": "ng build --configuration production",
    "build:stats": "ng build --configuration production --stats-json",
    "analyze": "webpack-bundle-analyzer dist/app-base/stats.json",
    "test": "ng test --code-coverage",
    "test:mutation": "stryker run && node utilidades/mutation/generate-mutation-sonar-report.js",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e:run": "ng run app-base:cypress-run --config-file ./cypress.e2e.config.ts --browser chrome",
    "bdd:run": "ng run app-base:cypress-run --config-file ./cypress.bdd.config.ts --browser chrome",
    "bdd": "ng e2e --config-file ./cypress.bdd.config.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.11",
    "@angular/cdk": "^13.3.9",
    "@angular/common": "~13.3.11",
    "@angular/compiler": "~13.3.11",
    "@angular/core": "~13.3.11",
    "@angular/forms": "~13.3.11",
    "@angular/localize": "^13.3.11",
    "@angular/material": "^13.3.9",
    "@angular/platform-browser": "~13.3.11",
    "@angular/platform-browser-dynamic": "~13.3.11",
    "@angular/router": "~13.3.11",
    "moment": "^2.29.4",
    "ngx-cookie-service": "~13.2.1",
    "protractor": "^7.0.0",
    "rxjs": "~7.5.5",
    "sweetalert2": "^11.6.7",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.7",
    "@angular-eslint/builder": "13.2.1",
    "@angular-eslint/eslint-plugin": "13.2.1",
    "@angular-eslint/eslint-plugin-template": "13.2.1",
    "@angular-eslint/schematics": "13.2.1",
    "@angular-eslint/template-parser": "13.2.1",
    "@angular/cli": "~13.3.7",
    "@angular/compiler-cli": "~13.3.11",
    "@angular/language-service": "~13.3.11",
    "@badeball/cypress-cucumber-preprocessor": "^11.5.1",
    "@cypress/schematic": "^2.0.0",
    "@cypress/webpack-preprocessor": "^5.12.0",
    "@faker-js/faker": "^7.6.0",
    "@stryker-mutator/core": "^6.0.2",
    "@stryker-mutator/karma-runner": "^6.0.2",
    "@stryker-mutator/typescript-checker": "^6.0.2",
    "@types/jasmine": "~4.0.3",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^17.0.38",
    "@typescript-eslint/eslint-plugin": "5.27.0",
    "@typescript-eslint/parser": "5.27.0",
    "cypress": "^10.3.0",
    "eslint": "^8.16.0",
    "eslint-plugin-rule-angular-arquitecture": "file:utilidades/eslint-plugin-rule-angular-arquitecture/",
    "jasmine": "^4.5.0",
    "jasmine-core": "~4.1.1",
    "jasmine-spec-reporter": "~7.0.0",
    "jest": "^29.3.1",
    "karma": "^6.3.20",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.1.0",
    "karma-jasmine": "~5.0.1",
    "karma-jasmine-html-reporter": "^2.0.0",
    "karma-junit-reporter": "^2.0.1",
    "license-checker": "^25.0.1",
